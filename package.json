{
  "name": "autostocks",
  "version": "1.0.0",
  "description": "Complete the inventory by adding the data in a Google Sheets.",
  "main": "./dist/app.js",
  "scripts": {
    "build": "npx tsc",
    "clean": "rm -rf ./dist",
    "dev": "nodemon -r dotenv/config --exec ts-node --files ./src/app.ts",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "prod": "NODE_ENV=production node .",
    "start": "npm run build && npm run prod"
  },
  "keywords": [
    "sheets",
    "google",
    "api",
    "rest",
    "stocks",
    "automation"
  ],
  "author": "Mathis Le Bonniec",
  "license": "GPLv3",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-spreadsheet": "^3.0.13",
    "helmet": "^4.2.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/google-spreadsheet": "^3.0.2",
    "@types/morgan": "^1.9.2",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^14.14.7",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
